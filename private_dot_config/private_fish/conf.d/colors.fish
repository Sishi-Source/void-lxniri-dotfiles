# Fluent-roundDark color scheme for Fish Shell

# General text and normal commands
set -g fish_color_normal '#dedede'

# Errors
set -g fish_color_error '#ff7b72'

# Parameters and arguments
set -g fish_color_param '#dedede'

# Functions
set -g fish_color_function '#dedede'

# Operators (e.g., in a command substitution)
set -g fish_color_operator '#8ab4f8'

# Quotes and strings
set -g fish_color_quote '#a5d6ff'

# Variables
set -g fish_color_variable '#8AB4F8'

# Comments
set -g fish_color_comment '#8b949e'

# Matching parentheses
set -g fish_color_match '#dedede'

# Command completions
set -g fish_color_completion '#dedede'

# Options (e.g., -h)
set -g fish_color_option '#a5d6ff'

# Redirections (e.g., >)
set -g fish_color_redirection '#a5d6ff'

# Status (e.g., success)
set -g fish_color_status '#58a6ff'

# The prompt
set -g fish_color_cwd '#8AB4F8'
set -g fish_color_user '#d2a8ff'
set -g fish_color_host '#ff7b72'
